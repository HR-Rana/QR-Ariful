{
    "version": 2,
    "builds": [
      {
        "src": "backend/**/*.js",  
        "use": "@vercel/node"   
      },
      {
        "src": "frontend/package.json", 
        "use": "@vercel/static-build" 
      }
    ],
    "routes": [
      { "src": "/backend/(.*)", "dest": "/backend/server.js" }, 
      
      { "src": "/(.*)", "dest": "/frontend/index.html" }   
    ]
  }
  